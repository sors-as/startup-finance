{
	"name": "1984-startup-finance-worker",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"description": "Real-time JSON API with Cloudflare Workers and Durable Objects",
	"scripts": {
		"deploy": "node scripts/deploy.js production",
		"deploy:staging": "node scripts/deploy.js staging",
		"deploy:production": "node scripts/deploy.js production",
		"dev": "wrangler dev --port 8787",
		"dev:local": "wrangler dev --config wrangler.local.toml --port 8787 --local",
		"dev:miniflare": "wrangler dev --config wrangler.local.toml --port 8787 --local",
		"start": "wrangler dev --port 8787",
		"start:local": "node miniflare.config.js",
		"db:init": "node scripts/init-db-simple.js",
		"db:setup-staging": "node scripts/setup-staging-db.js",
		"cf-typegen": "wrangler types",
		"lint": "echo 'No linter configured'",
		"typecheck": "tsc --noEmit",
		"test": "vitest"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.64",
		"@cloudflare/workers-types": "^4.20241218.0",
		"miniflare": "^3.20241218.0",
		"typescript": "^5.5.2",
		"vitest": "^3.2.4",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"lz-string": "^1.5.0"
	}
}
